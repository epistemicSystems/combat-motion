{
  "name": "combatsys-motion",
  "version": "0.1.0",
  "description": "Camera-only motion analysis platform for breathing, gait, and combat sports",
  "main": "out/main.js",
  "scripts": {
    "watch": "shadow-cljs watch main renderer",
    "compile": "shadow-cljs compile main renderer",
    "release": "shadow-cljs release main renderer",
    "start": "electron .",
    "dev": "concurrently \"npm run watch\" \"npm run start\"",
    "clean": "rm -rf out resources/public/js .shadow-cljs"
  },
  "keywords": [
    "motion-analysis",
    "pose-estimation",
    "breathing",
    "biomechanics"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "electron": "^28.0.0",
    "shadow-cljs": "^2.26.2"
  },
  "dependencies": {
    "@tensorflow/tfjs": "^4.15.0",
    "@tensorflow-models/pose-detection": "^2.1.3"
  }
}
